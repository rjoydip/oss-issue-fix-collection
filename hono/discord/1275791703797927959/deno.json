{
  "imports": {
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.2.2",
    "hono": "jsr:@hono/hono@^4.5.8",
    "zod": "npm:zod@^3.23.8"
  },
  "tasks": {
    "start:server": "deno run --allow-net server.ts",
    "start:client": "deno run --allow-net client.ts"
  },
  "compilerOptions": {
    "strict": true,
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
