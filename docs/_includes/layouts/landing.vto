    <div class="landing-install">
      {{ install.title |> md }}

      <div class="terminal">
        <pre><code>{{ install.code }}<lume-copy class="terminal-copy"><button data-text="{{ install.code }}"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 256 256"><path d="M216,32H88a8,8,0,0,0-8,8V80H40a8,8,0,0,0-8,8V216a8,8,0,0,0,8,8H168a8,8,0,0,0,8-8V176h40a8,8,0,0,0,8-8V40A8,8,0,0,0,216,32ZM160,208H48V96H160Zm48-48H176V88a8,8,0,0,0-8-8H96V48H208Z"></path></svg></button></lume-copy></code></pre>
        <a class="terminal-cta" href="{{ install.url }}" aria-label="Continue to Getting Started">
          <img src="/icons/arrow-right.svg" inline>
        </a>
      </div>

      <lume-shield data-name="denoland/lume"></lume-shield>
        {{ for badge of badges }}
          <lume-shield data-name="{{ badge.name }}" data-url="{{ badge.url }}" data-version="{{ badge.version }}" data-color="{{ badge.color }}" data-skip-nudd></lume-shield>
        {{ /for }}
      </div>
  </header>

  <section class="landing-usage">
  {{ for block of usage }}
    <div class="landing-usage-text">
      <h2>{{ block.title }}</h2>
      {{ block.description |> md }}
    </div>
    <div class="landing-usage-code">
      {{ block.code |> md }}
    </div>
  {{ /for }}
  </section>

  <section class="landing-contribute">
    <h2>{{ contribute.title }}</h2>

    <div>
      <ul class="landing-contributors">
        {{ for contributor of contribute.contributors }}
        <li>
          <a href="{{ contributor.url }}" title="{{ contributor.name }}">
            <img src="{{ contributor.img }}" srcset="{{ contributor.img }}, {{ contributor.img |> replace(".webp", "@2x.webp") }} 2x" height="100" width="100" alt="{{ contributor.name }}">
          </a>
        </li>
        {{ /for }}
      </ul>
    </div>

    <div class="landing-contribute-guideline">
      <h3>{{ contribute.guideline.title }}</h3>
      {{ contribute.guideline.description |> md }}
    </div>
  </section>
</div>

<style>
  .badges {
    position: relative;
    width: 15em;
    height: 15em;
    overflow: hidden;
    border-radius: 100%;
    filter: url(#badges-blur);
  }
  .badge,
  .badge::after {
    position: absolute;
    border-radius: 100%;
    animation-fill-mode: backwards;
    animation-duration: 1.2s;
    animation-timing-function: cubic-bezier(0.5, 0.07, 0.64, 1);
    animation-iteration-count: infinite;
  }
  .badge {
    left: 12%;
    bottom: 0;
    width: 5em;
    height: 5em;
    animation-name: firecircle;
    will-change: transform;
  }
  .badge::after {
    content: "";
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    animation-name: fireafter;
    will-change: opacity;
    opacity: 0;
  }
  .badge:nth-child(1),
  .badge:nth-child(1)::after {
      animation-delay: -0.14s;
      animation-duration: 5s; /* Adjust animation duration as needed */
  }
  .badge:nth-child(1) {
      margin-left: 1em;
      margin-bottom: 2.2em;
  }
  .badge:nth-child(2),
  .badge:nth-child(2)::after {
      animation-delay: -0.28s;
      animation-duration: 5s; /* Adjust animation duration as needed */
  }
  .badge:nth-child(2) {
      margin-left: -1.1em;
      margin-bottom: 1.2em;
  }
  .badge:nth-child(3),
  .badge:nth-child(3)::after {
      animation-delay: -0.42s;
      animation-duration: 5s; /* Adjust animation duration as needed */
  }
  .badge:nth-child(3) {
      margin-left: 6.8em;
      margin-bottom: 1.8em;
  }
  .badge:nth-child(4),
  .badge:nth-child(4)::after {
      animation-delay: -0.56s;
      animation-duration: 5s; /* Adjust animation duration as needed */
  }
  .badge:nth-child(4) {
      margin-left: 4.5em;
      margin-bottom: 3em;
  }
  .badge:nth-child(5),
  .badge:nth-child(5)::after {
      animation-delay: -0.8s;
      animation-duration: 5s; /* Adjust animation duration as needed */
  }
  .badge:nth-child(5) {
      margin-left: 2.8em;
      margin-bottom: 1.1em;
  }
  @keyframes firecircle {
    0% {
      transform: translateY(0) scale(1);
    }

    100% {
      transform: translateY(-11em) scale(0);
    }
  }
